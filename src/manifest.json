{
  "manifest_version": 2,
  "name": "Real Estate Agent",
  "version": "0.0.1",
  "options_page": "options.html",
  "browser_action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.realestate.com.au/property*"
      ],
      "js": [
        "content-script.bundle.js"
      ]
    }
  ]
}